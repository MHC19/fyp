            <learn>scripts/chatbot/aiml_files/bot_reductions/atomic-categories0.aiml</learn>

            <!-- Second load
            Note: Missing yesno.aiml -->
            <learn>scripts/chatbot/aiml_files/my_info_tabby/client_profile.aiml</learn>
            <learn>scripts/chatbot/aiml_files/my_info_tabby/salutations.aiml</learn>

